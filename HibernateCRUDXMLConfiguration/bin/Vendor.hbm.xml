<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.tadigital.entity.Vendor" table="vendor_information">
		<id name="id" column="vendor_id">
			<generator class="native"/>
		</id>
		<property name="name" column="vendor_name"/>
		<property name="email" column="vendor_email"/>
		<property name="password" column="vendor_password"/>
	</class>
</hibernate-mapping>